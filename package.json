    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "@types/babel-types": "^7.0.9",
    "@types/jest": "^26.0.14",
    "@types/ramda": "^0.27.23",
    "@types/react": "^16.9.51",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
