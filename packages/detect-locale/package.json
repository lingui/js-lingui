{
  "name": "@lingui/detect-locale",
  "version": "4.1.0",
  "sideEffects": false,
  "description": "@Lingui package to help you find the correct browser/server locale",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "i18n",
    "i10n",
    "i9n",
