{
  "name": "@lingui/cli",
  "version": "4.8.0-next.1",
  "description": "CLI for working wit message catalogs",
  "keywords": [
    "cli",
    "i18n",
    "internationalization",
    "i10n",
    "localization",
    "i9n",
    "translation",
    "multilingual"
  ],
  "repository": "lingui/js-lingui",
  "bugs": "https://github.com/lingui/js-lingui/issues",
  "license": "MIT",
  "author": {
    "name": "Tomáš Ehrlich",
    "email": "tomas.ehrlich@gmail.com"
  },
  "bin": {
    "lingui": "./dist/lingui.js"
  },
  "exports": {
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    },
    "./api/extractors/babel": {
      "types": "./dist/api/extractors/babel.d.ts",
      "default": "./dist/api/extractors/babel.js"
    },
    "./api/extractors/typescript": {
      "types": "./dist/api/extractors/typescript.d.ts",
      "default": "./dist/api/extractors/typescript.js"
    }
  },
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ],
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/generator": "^7.21.1",
    "@babel/parser": "^7.21.2",
    "@babel/runtime": "^7.21.0",
    "@babel/types": "^7.21.2",
    "@lingui/babel-plugin-extract-messages": "4.8.0-next.1",
    "@lingui/babel-plugin-lingui-macro": "4.8.0-next.1",
    "@lingui/conf": "4.8.0-next.1",
    "@lingui/core": "4.8.0-next.1",
    "@lingui/format-po": "4.8.0-next.1",
    "@lingui/message-utils": "4.8.0-next.1",
    "chalk": "^4.1.0",
    "chokidar": "3.5.1",
    "cli-table": "0.3.6",
    "commander": "^10.0.0",
    "convert-source-map": "^2.0.0",
    "date-fns": "^2.16.1",
    "esbuild": "^0.21.5",
    "glob": "^7.1.4",
    "inquirer": "^7.3.3",
    "micromatch": "4.0.2",
    "normalize-path": "^3.0.0",
    "ora": "^5.1.0",
    "pathe": "^1.1.0",
    "pkg-up": "^3.1.0",
    "pofile": "^1.1.4",
    "pseudolocale": "^2.0.0",
    "ramda": "^0.27.1",
    "source-map": "^0.8.0-beta.0"
  },
  "devDependencies": {
    "@lingui/jest-mocks": "*",
    "@types/convert-source-map": "^2.0.0",
    "@types/glob": "^8.1.0",
    "@types/micromatch": "^4.0.1",
    "@types/normalize-path": "^3.0.0",
    "mock-fs": "^5.2.0",
    "mockdate": "^3.0.5"
  }
}
