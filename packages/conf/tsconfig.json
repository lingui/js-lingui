{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2019",
    "sourceMap": true,
    "noEmit": false,
    "declaration": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": ["vitest/globals"]
  }
}
