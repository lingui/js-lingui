  },
  "dependencies": {
    "@lingui/babel-plugin-lingui-macro": "workspace:*",
    "@lingui/core": "workspace:*"
  },
  "devDependencies": {
    "@lingui/test-utils": "workspace:*",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.13",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unbuild": "3.6.1",
    "vitest": "4.0.18"
  }
